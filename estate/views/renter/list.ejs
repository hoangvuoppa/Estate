<!-- Thanh tim kiem -->
<div class="container-fluid parent-search">
  <h1 class="wow tada">FIND YOUR LOCATION</h1>
  <div class="container search wow bounceIn">
    <div class="row row1">
      <div class="col-sm-1"></div>
      <div class="col-sm-2 col-6">
        <div>Loại phòng</div>
        <select>
          <option>Tất cả</option>
          <option>Nhà trọ</option>
          <option>Chung cư mini</option>

        </select>
      </div>
      <div class="col-sm-2  col-6">
        <div>Tỉnh/Thành phố</div>
        <select>
          <option value="Tất cả">Tất cả</option>
          <option value="">Hồ Chí Minh</option>
        </select>
      </div>
      <div class="col-sm-2  col-6">
        <div>Quận/Huyện</div>
        <select>
          <option>Tất cả</option>
          <option value="">Bà Rịa Vũng Tàu</option>

        </select>
      </div>
      <div class="col-sm-2  col-6">
        <div>Phường/Xã</div>
        <select>
          <option>Tất cả</option>
          <option>Tân Sơn Nhất</option>

        </select>
      </div>
      <div class="col-sm-2  col-6">
        <div>Đường/Phố</div>
        <select>
          <option>Nguyễn Đình Chiểu</option>

        </select>
      </div>
      <div class="col-sm-1"></div>
    </div>
    <div class="row row1">
      <div class="col-sm-3"></div>
      <div class="col-sm-2  col-6">
        <div>Diện tích</div>
        <select>
          <option>Tất cả</option>
          <option>15m2 - 20m2</option>
        </select>
      </div>
      <div class="col-sm-2  col-6">
        <div>Mức giá</div>
        <select>
          <option>Tất cả</option>
          <option>1 triệu - 3 triệu</option>
        </select>
      </div>
      <div class="col-sm-2  col-6">
        <div>An ninh</div>
        <select>
          <option>Tất cả</option>
          <option>Chung chủ</option>
          <option>Không chung chủ</option>
        </select>
      </div>
      <div class="col-sm-3"></div>
    </div>
    <div class="row row2">
      <div class="col-sm-1"></div>
      <div class="col-sm-2 col-4">
        <input type="checkbox"></input>
        <img class="img-fluid w-50 " src="/images/dieu-hoa.png" alt="">
      </div>
      <div class="col-sm-2 col-4">
        <input type="checkbox"></input>
        <img class="img-fluid w-50" src="/images/binh-nong-lanh.png" alt="">
      </div>
      <div class="col-sm-2 col-4">
        <input type="checkbox"></input>

        <img class="img-fluid w-25" src="/images/tu-lanh.png" alt="">
      </div>
      <div class="col-sm-2 col-4">
        <input type="checkbox"></input>
        <img class="img-fluid w-50" src="/images/may-giat.png" alt="">
      </div>
      <div class="col-sm-2 col-4">
        <input type="checkbox"></input>
        <img class="img-fluid w-50" src="/images/lo-suoi.png" alt="">
      </div>
      <div class="col-sm-1"></div>
    </div>

  </div>
  <div id="search" class="wow fadeInDown">SEARCH</div>

</div>




<%include ./header-footer/header %>

<!-- Form -->
<div class="col-md-6 col-sm-4 col-xs-6 card">

  <h5 class="card-header info-color white-text text-center py-4">
    <strong>Login</strong>
  </h5>

  <!--Card content-->
  <div class="card-body px-lg-5 pt-0  text-center" style="color: #757575;">

    <!-- Form -->
    <form>
      <!-- Email -->
      <div class="md-form">
        <label for="email">E-mail</label>
        <input type="email" id="email" class="form-control">
      </div>
      <!-- Password -->
      <div class="md-form">
        <label for="password">Password</label>
        <input type="password" id="password" class="form-control">
      </div>
      <!-- Sign in button -->
      <button class="btn btn-outline-info btn-rounded btn-block my-4 waves-effect z-depth-0" id="btn">Login</button>

      <!-- Register -->
      <p>Not a member?
        <a href="/register">Register</a>
      </p>
    </form>
  </div>
</div>
<!-- Form -->

<%include ./header-footer/footer %>